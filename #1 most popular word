import java.io.*;
import java.util.*;


  public String getTheMostPopularWord(List<String> input){
    String res = "";
    int maxFreq = -1;
    
    Map<String, Integer> map = new HashMap<>();
    
    
    for(int i = 0 ; i < input.size(); i++){
      String text = input.get(i);
      String[] words = text.split("\\s+");
    
      for(String word : words){
          map.put(word, map.getOrDefault(word,0)+1);
        
          int curFreq = map.get(word);
        
          if (curFreq > maxFreq) {
           maxFreq =  curFreq;
            res = word; 
          }
      } 
    }
  
    return res;
  }



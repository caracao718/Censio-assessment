class Solution {
  public static void main(String[] args) {
      
    Solution sol = new Solution();
    
    String input = "-???9******8???2";
    boolean res =sol.checkString(input);
    System.out.println(res);
    
  }
  
  
  public boolean checkString(String input){
    String target = "???";
    List<Integer> indexs = new ArrayList<>();
    
    for (int i = 1; i < input.length() - 3; i++) { 
        if(input.substring(i,i+3).equals(target)){
          indexs.add(i);
        }  
    }
    
    
    for(Integer index: indexs){
      int i = Character.getNumericValue(input.charAt(index-1)); 
      int j = Character.getNumericValue(input.charAt(index+3)); 
      if (i+j == 10) return true;
    }
    
    
    return false;
  }

